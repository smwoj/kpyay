{
  "name": "hotloader",
  "version": "1.6.0",
  "description": "A simple template for creating react app using TypeScript with hot reload.",
  "main": "index.js",
  "scripts": {
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}' --exclude 'src/tests/*.{ts,tsx}'",
    "start": "webpack-dev-server",
    "test": "jest",
    "build": "tsc && webpack --progress",
    "clean": "rd /s/q built",
    "prepare": "tsc && webpack --progress --config webpack.prod.config.js"
  },
  "author": "Chun Liu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/chunliu/typescript-react-hot-reload"
  },
  "dependencies": {
    "antd": "^3.26.13",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-redux": "^7.2.0",
    "react-router": "5.1.2",
    "react-router-config": "5.1.1",
    "react-router-dom": "5.1.2",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.5",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/jest": "^25.1.4",
    "@types/node": "^12.12.30",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "@types/react-router": "^5.1.4",
    "@types/react-router-config": "^5.0.1",
    "@types/react-router-dom": "^5.1.3",
    "css-loader": "^3.4.2",
    "enzyme": "^3.11.0",
    "file-loader": "^5.1.0",
    "hoist-non-react-statics": "^3.3.2",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.1.0",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "react-test-renderer": "^16.13.0",
    "redux-devtools-extension": "^2.13.8",
    "source-map-loader": "^0.2.4",
    "ts-import-plugin": "^1.6.5",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.1",
    "tslint": "^6.1.0",
    "tslint-loader": "^3.5.4",
    "typescript": "^3.8.3",
    "url-loader": "^3.0.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/tests/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
